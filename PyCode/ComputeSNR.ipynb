   "source": [
    "def ComputeSNR(s, n):\n",
    "    s = np.asanyarray(s)\n",
    "    n = np.asanyarray(n)\n",
    "    snr = 10 * np.log10(np.mean(s**2)/np.mean(n**2))\n",
    "    return snr"
   ]
  }
