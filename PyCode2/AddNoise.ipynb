   "source": [
    "def AddNoise(target_snr_db, s):\n",
    "    s = np.asanyarray(s)\n",
    "    var_n = np.mean(s**2) / (10**(target_snr_db/10))\n",
    "    n = np.random.normal(0, np.sqrt(var_n), len(s))    \n",
    "    s_n = s + n # noise up the original signal\n",
    "    return s_n, n, var_n"
   ]
  }
