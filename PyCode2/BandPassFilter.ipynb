    "def BandPassFilter(signal, k_BP, fc):\n",
    "    alpha = (1 - k_BP*np.cos(2*np.pi*fc) - np.sqrt(2*k_BP*(1-np.cos(2*np.pi*fc)) - k_BP**2 * np.sin(2 * np.pi * fc)**2))/(1-k_BP)\n",
    "    signal_f = scsig.filtfilt(1-alpha, [1, -alpha], signal)\n",
    "    return(signal_f)"
   ]
