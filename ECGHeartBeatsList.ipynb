    "def ECGHeartBeatsList(ecg_bwr, mids):\n",
    "    x_hbs = []\n",
    "    for i in range(len(mids)-1):\n",
    "        x_hb = ecg_bwr[mids[i]:mids[i+1]]\n",
    "        x_hbs.append(x_hb)\n",
    "    return(x_hbs)    "
   ]
  }
