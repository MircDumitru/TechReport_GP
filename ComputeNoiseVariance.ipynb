   "source": [
    "def ComputeNoiseVariance(x, mu_xph):\n",
    "    var_n_ME = np.abs(sum(i*i for i in x) - sum(i*i for i in mu_xph)[0]) / (len(x)-len(mu_xph))\n",
    "    return var_n_ME"
   ]
  }
